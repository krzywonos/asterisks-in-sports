        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#PrimaryAttitude"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#SecondaryAttitude"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#isInfluenced -->

