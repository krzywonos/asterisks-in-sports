        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#GoverningBody"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#participatedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#participatedIn">
