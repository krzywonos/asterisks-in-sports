        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Context"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#hasViewpoint -->

