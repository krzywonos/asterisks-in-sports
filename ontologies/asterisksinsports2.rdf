     -->

    


    <!-- http://purl.org/dc/terms/Location -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Location">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Location"/>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude">
        <rdfs:label>Attitude</rdfs:label>
        <rdfs:label>The behavior of an Actor towards the result of a competition.</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#BackgroundKnowledge -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#BackgroundKnowledge">
        <rdfs:comment>The factors that inform the decision or opinion of an Actor. 

They can be cultural, technological, historical, social, political, or economic.</rdfs:comment>
        <rdfs:label>Background knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#CognitivePerspectivization -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#CognitivePerspectivization">
        <rdfs:label>Cognitive perspectivization</rdfs:label>
        <rdfs:label>Represents the moment where and the motivations for Actors change an Attitude.</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Eventuality -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Eventuality"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens">
        <rdfs:comment>Represent all the factors that inform the decision or opinion making of an Actor.</rdfs:comment>
        <rdfs:label>Lens</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Achievement -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Achievement">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Eventuality"/>
        <rdfs:comment>Represents a sporting accomplishment made by a sportsperson or a group of sportspeople. This ranges from winning championship titles, through outstanding performances and breaking records to just qualifying to participate at a certain event.</rdfs:comment>
        <rdfs:label>Achievement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#AttitudeShift -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#AttitudeShift">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#CognitivePerspectivization"/>
        <rdfs:comment>Represents a personal shift of attitute regarding a sporting achievement.</rdfs:comment>
        <rdfs:label>Attitude shift</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Context -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Context">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#BackgroundKnowledge"/>
        <rdfs:comment>Represents context and a plethora of external factors (e.g. media coverage, personal knowledge, cultural and moral values) influencing outlooks on a sporting achievement.</rdfs:comment>
        <rdfs:label>Context</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#ContextualFactors -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#ContextualFactors">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents the cases, where the legitimacy of an achievement has been challenged due to factors outside of the actual sporting results.</rdfs:comment>
        <rdfs:label>Contextual factors</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy">
        <rdfs:comment>Represents the type of controversy surrounding a sporting achievement.</rdfs:comment>
        <rdfs:label>Controversy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Doping -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Doping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversiues due to improving performance through illicit chemicals.</rdfs:comment>
        <rdfs:label>Doping</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Ethics -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Ethics">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversies due to ethical considerations.</rdfs:comment>
        <rdfs:label>Ethics</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Gender -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Gender">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversies due to athletes gender/hormone levels.</rdfs:comment>
        <rdfs:label>Gender issues</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#GoverningBody -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#GoverningBody">
        <owl:equivalentClass rdf:resource="https://sportschema.org/ontologies/main/GoverningBody"/>
        <rdfs:comment>Represents the governing body, under whose auspices the event was run, in which an achievement has been made in.</rdfs:comment>
        <rdfs:label>Governing body</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Location -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Location">
        <rdfs:comment>Represents the singular physical location (if possible), or a broader geographical context, where an Achievement has taken place.</rdfs:comment>
        <rdfs:label>Location</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Officiating -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Officiating">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversies due to officiating mistakes and/or corrupt officials.</rdfs:comment>
        <rdfs:label>Officiating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Participant -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Participant">
        <owl:equivalentClass rdf:resource="https://sportschema.org/ontologies/main/Agent"/>
        <rdfs:comment>Representation of a specific sportsperson, or a group of sportspeople participating in a club or representing their nation in a sporting competition.</rdfs:comment>
        <rdfs:label>Participant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#PrimaryAttitude -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#PrimaryAttitude">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude"/>
        <rdfs:comment>The primary decision of an Actor, in regards to some Achievement.</rdfs:comment>
        <rdfs:label>Primary decision</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#RuleViolation -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#RuleViolation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversies due to violating the ruleset of the sport or competition.</rdfs:comment>
        <rdfs:label>Rule violation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#SecondaryAttitude -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#SecondaryAttitude">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude"/>
        <rdfs:comment>The secondary decision of an Actor, towards a certain Achievement, coming due to a change of viewpoint.</rdfs:comment>
        <rdfs:label>Secondary decision</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Sport -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Sport">
        <rdfs:comment>Represents the sport, in which an Achievement has been made in.</rdfs:comment>
        <rdfs:label>Sport</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Technology -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Technology">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Controversy"/>
        <rdfs:comment>Represents cases, where a sporting achievement has been surrounded by controversies due to technological advancements impacting the result.</rdfs:comment>
        <rdfs:label>Technological advancements</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#TimePeriod -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#TimePeriod">
        <owl:equivalentClass rdf:resource="https://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Represents the timeframe, when an Achievement has taken place.</rdfs:comment>
        <rdfs:label>Time period</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#Viewpoint -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Viewpoint">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens"/>
        <rdfs:comment>Represents a perspective of an individual towards a sporting achievement.</rdfs:comment>
        <rdfs:label>Viewpoint</rdfs:label>
    </owl:Class>
    


    <!-- https://sportschema.org/ontologies/main/Agent -->

    <owl:Class rdf:about="https://sportschema.org/ontologies/main/Agent">
        <rdfs:comment>The person or institution that competes on a competition.</rdfs:comment>
        <rdfs:label>Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://sportschema.org/ontologies/main/GoverningBody -->

    <owl:Class rdf:about="https://sportschema.org/ontologies/main/GoverningBody"/>
    


    <!-- https://www.w3.org/2006/time#TemporalEntity -->

    <owl:Class rdf:about="https://www.w3.org/2006/time#TemporalEntity">
        <rdfs:label>TemporalEntity</rdfs:label>
    </owl:Class>
    


