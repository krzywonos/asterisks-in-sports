        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/asterisksinsports#Sport"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2024/asterisksinsports#towards1 -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/asterisksinsports#towards1">
